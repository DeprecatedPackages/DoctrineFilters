services:
	- {class: Zenify\DoctrineFilters\Tests\Filters\ActiveFilter, tags: [zenify.doctrine.filter]}
	- {class: Zenify\DoctrineFilters\Tests\Filters\ConditionFilter, tags: [zenify.doctrine.filter]}

extensions:
	- Kdyby\Console\DI\ConsoleExtension
	- Kdyby\Events\DI\EventsExtension
	- Kdyby\Annotations\DI\AnnotationsExtension
	doctrine: Kdyby\Doctrine\DI\OrmExtension
	- Zenify\DoctrineFilters\DI\FiltersExtension


doctrine:
	driver: pdo_sqlite
	memory: true
